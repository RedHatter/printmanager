{
  "name": "workflow",
  "version": "4.3.0",
  "description": "Manages print jobs.",
  "main": "app.js",
  "author": "Timothy Johnson",
  "private": true,
  "scripts": {
    "start": "node app.js",
    "build": "yarn rasterize && yarn format && rollup -c",
    "format": "node --experimental-modules ./script/format.mjs",
    "rasterize": "mkdir -p ./public/images && for f in ./src/images/*; do d=./public/images/`basename $f .svg`.png && test ! -e $d -o $f -nt $d && inkscape $f -e $d; done; return 0",
    "build-production": "NODE_ENV=production yarn build",
    "clean": "rm -r public/*[^index.html] public/login/*[^index.html]",
    "deploy": "rsync -rtv --update --delete --exclude-from .rsyncignore . generic-MERN:/opt/printmanager"
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "aws-amplify": "^1.1.19",
    "cognito-express": "^2.0.12",
    "date-fns": "^2.0.0-alpha.27",
    "jimp": "^0.6.0",
    "koa": "^2.7.0",
    "koa-body": "^4.0.8",
    "koa-router": "^7.2.1",
    "koa-session": "^5.4.0",
    "koa-static": "^5.0.0",
    "mongoose": "^5.4.17",
    "mongoose-autopopulate": "^0.9.1",
    "mongoose-patch-history": "https://github.com/RedHatter/mongoose-patch-history.git#all-changes",
    "nodemailer": "^6.1.0",
    "socket.io": "^2.2.0",
    "xss": "^1.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.4",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@date-io/date-fns": "^1.1.0",
    "@dbeining/react-atom": "^4.1.0",
    "@material-ui/core": "^4.0.0-alpha.7",
    "@material-ui/styles": "^4.0.0-alpha.7",
    "bound-decorator": "^1.1.0",
    "clsx": "^1.0.3",
    "csscomb": "^4.2.0",
    "draft-js": "^0.10.5",
    "draft-js-export-html": "^1.3.3",
    "immer": "^2.1.2",
    "jwt-decode": "^2.2.0",
    "material-ui-chip-input": "^1.0.0-beta.15",
    "material-ui-pickers": "^3.0.0-alpha.1",
    "prettier": "^1.16.4",
    "prop-types": "^15.7.2",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-draggable": "^3.2.1",
    "react-number-format": "^4.0.6",
    "rollup": "^1.9.3",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^4.2.2",
    "rollup-plugin-re": "^1.0.7",
    "tiny-glob": "^0.2.6"
  }
}
